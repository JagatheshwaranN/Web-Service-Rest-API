<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite>
<suite name="API Test Suite" thread-count="5" parallel="tests">
	<listeners>
		<listener class-name="com.api.listener.TestListener" />
	</listeners>
	<test name="GET API Test">
		<classes>
			<class name="com.api.test.GetUserTest" />
		</classes>
	</test>
	<test name="POST API Test">
		<classes>
			<class name="com.api.test.AddUserTest" />
		</classes>
	</test>
	<test name="PUT API Test">
		<classes>
			<class name="com.api.test.UpdateUserTest" />
		</classes>
	</test>
</suite>
